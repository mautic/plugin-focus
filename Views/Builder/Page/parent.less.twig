.mf-page-iframe {
    position: fixed;
    z-index: 21005;
    top: 1px;
    right: 1px;
    left: 1px;
    bottom: 1px;
    width: 100%;
    height: 100%;
}

@media only screen and (max-width: 667px) {
    .mf-page-iframe {
        top: 0;
        right: 0;
        left: 0;
        bottom: 0;
    }
}
